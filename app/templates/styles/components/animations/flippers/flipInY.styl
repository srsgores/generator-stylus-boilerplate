@keyframes flipInY
	0%
		transform: perspective(em(400px)) rotateY(90deg)
		opacity: 0

	40%
		transform: perspective(em(400px)) rotateY(-10deg)

	70%
		transform: perspective(em(400px)) rotateY(10deg)

	100%
		transform: perspective(em(400px)) rotateY(0deg)
		opacity: 1

generateAnimationPlaceholder(flipInY)
